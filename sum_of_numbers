
import java.util.Scanner;
class sum_of_number{
    public static int calcSum(int a,int b,int c){
        return a+b+c;
    }
    public static void main(String args[]){
        Scanner sc=new Scanner(System.in);
        int T=sc.nextInt();
        for(int i=0;i<T;i++){
            int A=sc.nextInt();
            int B=sc.nextInt();
            int C=sc.nextInt();
           System.out.println(calcSum(A,B,C));
        }
    }
}
