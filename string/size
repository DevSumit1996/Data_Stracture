// String Length
#include<iostream>
#include<string.h>
using namespace std;
int main(){
    // string object
    string str="sumitkumar";
    
   cout<<str.size()<<endl;
   cout<<str.length()<<endl;
   cout<<strlen(str.c_str())<<endl;
   int i=0;
   while(str[i]!='\0'){
       i++;
   }
   cout<<i<<endl;
   for(int i=0;str[i]!='\0';i++);
   cout<<i<<endl;
    return 0;
}
